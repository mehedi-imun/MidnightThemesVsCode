  "tokenColors": [
    {
        "scope": [
            "comment",
            "punctuation.definition.comment"
        ],
        "settings": {
            "foreground": "#6A9955",
            "fontStyle": "italic"
        }
    },
    {
        "scope": [
            "constant",
            "entity.name.constant",
            "variable.other.constant"
        ],
        "settings": {
            "foreground": "#D19A66"
        }
    },
    {
        "scope": [
            "entity.name.type",
            "support.type"
        ],
        "settings": {
            "foreground": "#4EC9B0"
        }
    },
    {
        "scope": [
            "entity.name.function",
            "meta.function-call"
        ],
        "settings": {
            "foreground": "#DCDCAA"
        }
    },
    {
        "scope": [
            "keyword",
            "keyword.control",
            "keyword.operator"
        ],
        "settings": {
            "foreground": "#C586C0",
            "fontStyle": "bold"
        }
    },
    {
        "scope": [
            "storage",
            "storage.type"
        ],
        "settings": {
            "foreground": "#569CD6"
        }
    },
    {
        "scope": [
            "string",
            "string punctuation.definition.string"
        ],
        "settings": {
            "foreground": "#CE9178"
        }
    },
    {
        "scope": [
            "variable",
            "variable.other"
        ],
        "settings": {
            "foreground": "#9CDCFE"
        }
    },
    {
        "scope": [
            "variable.language"
        ],
        "settings": {
            "foreground": "#9CDCFE",
            "fontStyle": "italic"
        }
    },
    {
        "scope": [
            "invalid",
            "invalid.deprecated"
        ],
        "settings": {
            "foreground": "#F44747",
            "fontStyle": "bold"
        }
    },
    {
        "scope": [
            "punctuation",
            "meta.brace"
        ],
        "settings": {
            "foreground": "#D4D4D4"
        }
    },
    {
        "scope": [
            "meta.definition.variable",
            "support.variable"
        ],
        "settings": {
            "foreground": "#9CDCFE"
        }
    },
    {
        "scope": [
            "keyword.control.return",
            "keyword.control"
        ],
        "settings": {
            "foreground": "#FF6A6A",
            "fontStyle": "bold"
        }
    }
]

}
